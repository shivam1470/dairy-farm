{
  "expo": {
    "name": "Dairy Farm",
    "slug": "dairy-farm-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2e7d32"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dairyfarm.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2e7d32"
      },
      "package": "com.dairyfarm.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "dairyfarm",
    "plugins": [
      "expo-router"
    ]
  }
}
